/Container>\n                )}\n              </tbody>\n            </Table>\n          </Container>\n        </Col>\n      </Row>\n    </>\n  );\n};\n\nexport default ShowExpenses;\n","/home/dalvin/Documents/2020/Glints/Code/ga8-final2/src/components/AddCard.js",[],"/home/dalvin/Documents/2020/Glints/Code/ga8-final2/src/components/Status.js",["180","181"],"import React, { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport Cookies from \"js-cookie\";\nimport Loading from './Loading'\nimport {\n  Container,\n  Row,\n  Col,\n  Card,\n  CardBody,\n  CardText,\n  CardTitle,\n} from \"reactstrap\";\n\nfunction CheckStatus() {\n  const [results, setResults] = useState(\"\");\n  const [loading, setLoading] = useState(false);\n\n  const urlCard = \"http://3.0.91.163/card\";\n\n  const token = Cookies.get(\"token\");\n\n  console.log(results);\n\n  useEffect(() => {\n    axios\n      .get(urlCard, {\n        headers: { Authorization: `Bearer ${token}` },\n      })\n      .then((res) => {\n        console.log(res.data);\n        setResults(res.data);\n        setLoading(true);\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  }, []);\n\n  return (\n    <Container>\n      <Row>\n        {results.length !== 0 ? (\n          results.map((result) => {\n            if (result.saldo >= 1000000) {\n              return (\n                <Col md={4}>\n                  <Card\n                    className=\"mb-3\"\n                    style={{ backgroundColor: \"#bcf7f4\", color: \"#222222\" }}\n                  >\n                    <CardBody>\n                      <CardTitle>\n                        <strong>{result.cardBank}</strong>\n                      </CardTitle>\n                      <hr style={{ borderTop: \"2px solid #222222\" }} />\n                      <CardTitle>\n                        <strong>IDR {result.saldo}</strong>\n                      </CardTitle>\n                      <CardText>\n               